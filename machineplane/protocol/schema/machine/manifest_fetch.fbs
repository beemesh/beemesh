namespace beemesh.machine;

// Request to fetch a manifest from a peer
table ManifestFetchRequest {
    manifest_id: string;
    version: uint64;
    capability_token: string; // Base64 encoded capability token
}

// Response containing the fetched manifest
table ManifestFetchResponse {
    success: bool;
    manifest_data: [uint8]; // The encrypted manifest data
    version: uint64;
    error: string; // Error message if success is false
}

root_type ManifestFetchRequest;
